{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch current program",
			"program": "${file}",
			"request": "launch",
			"skipFiles": ["<node_internals>/**"],
			"type": "node"
		},
		{
			"name": "Discover current Nejafile",
			"request": "launch",
			"program": "${workspaceRoot}/.neja-build/bin/neja-dev",
			"args": ["-f", "${file}", "-C", "${fileDirname}/build/launch", "-m"],
			"type": "node"
		}
	]
}
